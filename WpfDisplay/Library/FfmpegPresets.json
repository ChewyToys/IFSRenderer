{
  "HQ h265 (.mp4)": "-c:v libx265 -crf 18 -preset veryslow -tag:v hvc1",
  "HQ prores (.mov)": "-c:v prores -profile:v 3 -qscale:v 21 -pix_fmt yuv422p10le",
  "Lossless h265 (.mp4)": "-c:v libx265 -x265-params lossless=1 -preset veryslow -tag:v hvc1",
  "Web h265 (.mp4)": "-movflags faststart -c:v libx265 -crf 32 -preset veryslow -tag:v hvc1",
  "Web VP9 (.webm)": "-c:v libvpx-vp9 -crf 40 -deadline best"
}